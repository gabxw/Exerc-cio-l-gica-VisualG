algoritmo "exercício_11"

var
   num, soma, contador : inteiro

inicio

   soma <- 0
   contador <- 1
   enquanto contador <= 7 faca
      escreva("Digite um número: ")
      leia(num)
      soma <- soma + num
      contador <- contador + 1
   fimenquanto

   escreva("A soma dos números é: ", soma)

fimalgoritmo
