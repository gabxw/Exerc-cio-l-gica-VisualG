algoritmo "exercício_9"
    var
        num1, num2, soma: inteiro
    inicio
        num1 := 6
        num2 := 100
        soma := 0
        enquanto num1 <= num2 faca
            soma := soma + num1
            num1 := num1 + 2
        fimenquanto
        escreva("A soma dos números é: ", soma)
fimalgoritmo
