algoritmo "Preenchimento de vetor"
    var
        vetor: vetor de inteiro(10)
    inicio
        para i de 1 ate 10 faca
            vetor[i] <- i * 2 - 1
        fimpara
        escreva("O vetor preenchido Ã©: ")
        para i de 1 ate 10 faca
            escreva(vetor[i], " ")
        fimpara
    fimalgoritmo